<%= form_for @projectt do |f| %>
    <p>
        <%= f.label :title %>
        <%= f.text_field :title %>
  
    </p>
  
     <p>
        <%= f.label :description %>
        <%= f.text_area :description %>
  
     </p>  

     <p>
        <%= f.label :start_date %>
        <%= f.text_field :start_date %>
  
     </p>

     <p>
        <%= f.label :end_date %>
        <%= f.text_field :end_date %>
  
     </p>

     <p>
        <%= f.label :cost_of_project %>
        <%= f.text_area :cost_of_project %>
  
     </p>

     <p>
        <%= f.label :status %>
        <%= f.select :status, Projectt.statuses.keys.to_a, prompt: "Select status" %>
  
     </p>
     
      
     <p>
        <%= f.submit %>
     </p>
  
<% end %>  